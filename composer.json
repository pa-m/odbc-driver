{
    "name": "pa-m/odbc-driver",
    "description": "odbc driver for laravel 5 based on ccovey and wajatimur works",
    "authors": [
        {
            "name": "Pascal Masschelier",
            "email": ""
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "5.x",
        "illuminate/database": "5.x",
        "ext-pdo_odbc": "*"
    },
    "require-dev": {
        "mockery/mockery": "dev-master"
    },
    "autoload": {
        "classmap": [
            "src/migrations"
        ],
        "psr-0": {
            "Foundation\\Database\\Driver": "src/"
        }
    },
    "minimum-stability": "dev"
}
